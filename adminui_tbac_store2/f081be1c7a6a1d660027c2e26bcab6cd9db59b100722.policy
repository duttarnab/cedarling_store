@id("Admin UI authorization policy")
permit (
  principal in Jans::Role::"api-admin",
  action in Jans::Action::"Execute",
  resource == Jans::Feature::"https://jans.io/oauth/jans-auth-server/session.readonly"
);

permit (
  principal in Jans::Role::"api-viewer",
  action in Jans::Action::"Execute",
  resource == Jans::Feature::"https://jans.io/oauth/jans-auth-server/session.readonly"
);